using System;
//Leer 5 números por teclado y calcular el promedio, luego imprimir el resultado.
//Para obtener el promedio, se realiza sumando las cantidades y luego se divide entre ellas.
namespace Gama5
{
    class Program
    {
        static void Main(string[] args)
        {
            float numero1, numero2, numero3, numero4, numero5, promedio;

            Console.WriteLine(" ~Ingrese los 5 números que desea promediar: ");
            numero1 = float.Parse(Console.ReadLine());
            numero2 = float.Parse(Console.ReadLine());
            numero3 = float.Parse(Console.ReadLine());
            numero4 = float.Parse(Console.ReadLine());
            numero5 = float.Parse(Console.ReadLine());

            Console.WriteLine($" ~ El promedio es de: {promedio = (numero1 + numero2 + numero3 + numero4 + numero5) / 5}");
        }
    }
}
